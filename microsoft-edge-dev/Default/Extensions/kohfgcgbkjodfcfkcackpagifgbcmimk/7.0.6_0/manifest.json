{
   "action": {
   },
   "background": {
      "service_worker": "main.js"
   },
   "commands": {
      "activate": {
         "description": "Activate to Start",
         "suggested_key": {
            "default": "Alt+R"
         }
      },
      "forward": {
         "description": "Next"
      },
      "goToTabBeingRead": {
         "description": "Go to Tab Being Read"
      },
      "play": {
         "description": "Pause/Resume"
      },
      "readSelection": {
         "description": "Highlight to Read"
      },
      "rewind": {
         "description": "Previous"
      },
      "speaker": {
         "description": "Speaker",
         "suggested_key": {
            "default": "Alt+S"
         }
      },
      "stop": {
         "description": "Stop"
      },
      "toggleShowReadIcon": {
         "description": "Toggle Show Read Selection Icon"
      }
   },
   "content_scripts": [ {
      "css": [ "injected/nr-ext-dom/nr-ext-dom.css" ],
      "matches": [ "\u003Call_urls>" ]
   }, {
      "all_frames": true,
      "js": [ "injected/nr-ext-auth.js" ],
      "matches": [ "*://*.naturalreaders.com/login-service/*" ]
   } ],
   "content_security_policy": {
      "object-src": "self",
      "script-src": "self"
   },
   "description": "Read aloud any text with realistic AI voices, compatible with webpages, kindle Ebooks, Google Docs, PDF, Emails, and more.",
   "externally_connectable": {
      "matches": [ "*://*.naturalreaders.com/*" ]
   },
   "homepage_url": "https://www.naturalreaders.com/online/",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "assets/img/128N.png",
      "32": "assets/img/32N.png",
      "48": "assets/img/48N.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "NaturalReader - AI Text to Speech",
   "permissions": [ "scripting", "activeTab", "contextMenus", "storage", "tts", "ttsEngine", "webNavigation", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "7.0.6",
   "web_accessible_resources": [ {
      "extension_ids": [  ],
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "assets/*", "injected/*", "download/*" ]
   } ]
}
