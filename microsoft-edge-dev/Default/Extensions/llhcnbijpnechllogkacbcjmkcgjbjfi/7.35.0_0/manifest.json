{
   "action": {
      "default_icon": {
         "38": "res/icon.png"
      },
      "default_title": "__MSG_BrowserActionTitle__"
   },
   "author": "Evernote",
   "background": {
      "service_worker": "ServiceWorkerExtension.js"
   },
   "content_scripts": [ {
